{
  "expo": {
    "name": "Myapp",
    "slug": "Myapp",
    "version": "1.0.0",
    "scheme": "app-scheme",
    "orientation": "portrait",
    
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {

      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {

        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {

    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}